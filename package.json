{
  "name": "math-camp-19-boardgame",
  "author": {
    "name": "Andy Tan",
    "email": "andytan0727@gmail.com"
  },
  "description": "Board game designed for UUM Math Camp 2019.",
  "version": "2.0.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^3.4.0",
    "chokidar": "^2.0.4",
    "cuid": "^2.1.4",
    "dotenv": "^6.2.0",
    "electron-is-dev": "^1.0.1",
    "immer": "^1.12.1",
    "konva": "^2.6.0",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-flip-toolkit": "^6.5.5",
    "react-konva": "^16.7.1",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "react-sortable": "^2.0.0",
    "react-spring": "^7.2.10",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0",
    "typescript": "^3.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:croco": "craco start",
    "build:croco": "craco build",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start:croco \" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "yarn build:croco",
    "electron-pack": "build --win"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "math-camp-19-boardgame",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    }
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-motion": "^0.0.27",
    "@types/react-redux": "^7.0.0",
    "@types/redux-logger": "^3.0.6",
    "@types/webpack-env": "^1.13.6",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "electron-devtools-installer": "^2.2.4",
    "wait-on": "^3.2.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2"
  }
}
